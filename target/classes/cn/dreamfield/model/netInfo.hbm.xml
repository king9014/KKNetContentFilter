<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.dreamfield.model.NetInfo" table="net_info">
		<id name="infoId" column="INFO_ID">
			<generator class="native"/>
		</id>
		<property name="infoName" column="INFO_NAME"/>
		<property name="infoWebsite" column="INFO_WEBSITE"/>
		<property name="infoCategory" column="INFO_CATEGORY"/>
		<property name="infoHtmlUrl" column="INFO_HTML_URL"/>
		<property name="infoOriginUrl" column="INFO_ORIGIN_URL"/>
		<property name="infoImgUrl" column="INFO_IMG_URL"/>
		<property name="infoDate" column="INFO_DATE"/>
		<property name="infoIntro" column="INFO_INTRO"/>
		<property name="infoCid" column="INFO_CID"/>
		<property name="infoStatus" column="INFO_STATUS"/>
		
	</class>
</hibernate-mapping>